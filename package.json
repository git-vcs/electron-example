{
  "name": "myapp",
  "version": "0.1.9",
  "description": "This is an description",
  "main": "public/electron.js",
  "author": {
    "email": "test@test.no"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.0.11",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "npm": "^6.14.6"
  },
  "scripts": {
    "buildIntallers": "npm run-script react-build && npm run-script packaging ",
    "react-build": "react-scripts build",
    "packaging": "electron-builder",
    "rpiInstaller": "npm run-script react-build && electron-builder --linux deb --armv7l ",
    "buildWindwos":"npm run-script react-build && electron-builder --win portable"
  },
  "eslintConfig": {
    "extends": "react-app",
    "productName": "exsample"
  },
  "build": {
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "armv7l",
            "x64",
            "ia32"
          ]
        }
      ],
      "category": "Game"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "files": [
      "**/*",
      "!src${/*}"
    ]
  },
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
